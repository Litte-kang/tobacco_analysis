<script type="text/javascript">
var useage;

$('document').ready(function(){

	var table = $('#table').dataTable({
        "scrollCollapse": true,
        'paging': false,
        'searching': false
    });

	useage = $('#useage').dataTable({
        "scrollCollapse": true,
        'paging': false,
        'searching': false,
        columns: [
        	{data: 'freshAmount'},
        	{data: 'dryAmount'},
        	{data: 'count'},
        	{data: 'amountTime'}
        ]
    });



	var url = '/analysis_room_usage?code=<%= role %>';
    $.getJSON(url, function(data){
    	useage.api().rows.add(data).draw();
    })
})

</script>