<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <h1 class="page-header">烤烟概览</h1>
   <div class="row">
		<h5 class="sub-header" style="text-align:center">烘烤概览</h5>
        <h6 style="text-align:center">单位：公斤</h6>
        <div class="table-responsive">
          <table class="table table-striped table-hover table-condensed" id="table">
              <thead>
                <tr>
                  <th rowspan="2">应用烤房数</th>
                  <th colspan="2">正在烘烤烟叶</th>
                  <th colspan="2">烘烤正常</th>
                  <th colspan="3">有质量争议</th>
                </tr>
                <tr>
                	<th>烤房数</th>
                	<th>鲜烟总量</th>
                	<th>房数</th>
                	<th>干烟总量</th>
                	<th>房数</th>
                	<th>烟叶总量</th>
                	<th>已仲裁房数</th>
                </tr>
              </thead>
              <tbody>
              	<%_.each(result, function(p){ %>
              		<tr>
              			<td>50</td>
              			<td><%= p.bakingRoom %></td>
              			<td><%= p.amount %></td>
              			<td><%= p.normalRoom %></td>
              			<td><%= p.dryAmount %></td>
              			<td><%= p.abitrateRoom %></td>
              			<td><%= p.abitrateTobacco %></td>
              			<td><%= p.abitrateFinished %></td>
              		</tr>
              	<% }) %>
              </tbody>
            </table>
  		</div>
	</div>
</div>
<script type="text/javascript">
var table;
$('document').ready(function(){
	table = $('#table').dataTable({
        "scrollCollapse": true
    });
})

</script>