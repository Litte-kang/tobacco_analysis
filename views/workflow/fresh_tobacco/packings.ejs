          <h5 class="sub-header">烘烤鲜烟装烟情况查询表</h5>
          <h6>单位：公斤</h6>
          <div class="table-responsive">
            <table class="table table-striped table-hover table-condensed">
              <thead>
                <tr>
                  <th>烤房编号</th>
                 
                  <th>烟基号</th>
                  <th>鲜烟总量</th>
                  <th>装烟量</th>
                  <th>平均重量</th>
                  <th>同杆同质</th>
                  <th>混编</th>
                  <th>均匀</th>
                  <th>前密后稀</th>
                  <th>前稀后密</th>
                  <th>同层同质</th>
                  <th>按气流方式正确分层</th>
                  <th>各竿/夹量基本一致</th>
                  <th>各竿/夹量不一致</th>
                  <th>同竿/夹均匀</th>
                  <th>同竿/夹不匀</th>
                </tr>
              </thead>
              <tbody>
               	<% _.each(results, function(tobacco) { %>
               		<tr>
               		 
               		 <td><%= tobacco.room %></td>
               		 <td><%= tobacco.tobacco_no %></td>
               		 <td><%= tobacco.amount / 2 %></td>
                   <td><%= tobacco.bars %></td>
               		 <td><%= (tobacco.amount / tobacco.bars/ 2).toFixed(2) %></td>
                   <td><%= tobacco.totalA /2 %></td>
               		 <td><%= tobacco.totalB /2 %></td>
                   <td><%= tobacco.totalC /2 %></td>
               		 <td><%= tobacco.totalD /2 %></td>
               	   <td><%= tobacco.totalE /2 %></td>
                   <td><%= tobacco.totalF /2 %></td>
                   <td><%= tobacco.totalG /2 %></td>
                   <td><%= tobacco.totalH /2 %></td>
                   <td><%= tobacco.totalI /2 %></td>
                   <td><%= tobacco.totalJ /2 %></td>
                   <td><%= tobacco.totalK /2 %></td>
               		</tr>
    			     <% }) %>
              </tbody>
            </table>
           
